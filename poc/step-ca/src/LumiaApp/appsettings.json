{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "StepCA": {
    "BaseUrl": "https://step-ca:9000",
    "RootCertPath": "/app/certs/root_ca.crt"
  },
  "ProvisioningService": {
    "BaseUrl": "https://provisioning-service:5001"
  },
  "MQTT": {
    "BrokerHost": "mosquitto",
    "BrokerPort": "8883",
    "ClientId": "lumia-app"
  },
  "Certificate": {
    "RenewalThresholdDays": 7,
    "CheckIntervalMinutes": 60
  }
}